  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm i",
    "build": "tsc -p src && tsc -p benchmark",
    "build:watch": "tsc --watch -p src && tsc --watch -p benchmark",
    "pretest": "npm run build",
    "test": "ts-mocha --forbid-only -p test/tsconfig.json test/*.ts",
    "autoformat": "prettier --write src test typings benchmark",
    "autoformat:check": "prettier --check src test/*.ts typings benchmark/*.ts",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
