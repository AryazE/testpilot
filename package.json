{
  "name": "testpilot",
  "version": "0.0.1",
  "description": "Test generation using large language models",
  "main": "dist/index.js",
  "author": "Max Schaefer <max-schaefer@github.com>",
  "contributors": [
    "Frank Tip <f.tip@northeastern.edu>",
    "Sarah Nadi <nadi@ualberta.ca>",
