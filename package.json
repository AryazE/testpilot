    "@types/levenshtein": "^1.0.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.20.55",
    "@types/yargs": "^17.0.10",
    "chai": "^4.3.6",
    "deep-equal-in-any-order": "^2.0.0",
    "prettier": "^2.7.1",
